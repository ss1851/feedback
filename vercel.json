{
  "version": 2,
  "routes": [
    {
      "src": "/api/feedback",
      "dest": "/api/feedback.js"
    },
    {
      "src": "^/$",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/cards.html",
      "dest": "/frontend/cards.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
